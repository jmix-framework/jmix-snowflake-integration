<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://jmix.io/schema/ui/window"
        caption="msg://sandboxScreen.caption">
    <layout>
        <groupBox id="sequencesGroupBox" caption="Sequences" spacing="true">
            <textField id="sequenceNameField"/>
            <buttonsPanel>
                <button id="getNextValBtn" caption="Get next value"/>
                <button id="getCurValBtn" caption="Get current value"/>
                <button id="resetSeqBtn" caption="Reset sequence"/>
                <button id="dropSeqBtn" caption="Drop sequence"/>
            </buttonsPanel>
            <textArea id="outputField" width="100%" height="500px"/>
        </groupBox>
        <groupBox id="dateTimeGroupBox">
            <button id="loadAllDateTimeEntitiesBtn" caption="Load all"/>
        </groupBox>

    </layout>
</window>